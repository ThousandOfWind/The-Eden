<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_daily">

    <fragment
        android:id="@+id/navigation_daily"
        android:name="com.example.theedenv3.ui.daily.DailyFragment"
        android:label="@string/title_daily"
        tools:layout="@layout/daily_fragment" />

    <fragment
        android:id="@+id/navigation_town"
        android:name="com.example.theedenv3.ui.home.TownFragment"
        android:label="@string/title_town"
        tools:layout="@layout/fragment_town" />

    <fragment
        android:id="@+id/navigation_friends"
        android:name="com.example.theedenv3.ui.friends.FriendsFragment"
        android:label="@string/title_friends"
        tools:layout="@layout/fragment_friends" />

    <fragment
        android:id="@+id/navigation_person"
        android:name="com.example.theedenv3.ui.person.PersonalFragment"
        android:label="@string/title_person"
        tools:layout="@layout/personal_fragment" />
</navigation>
